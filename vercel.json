{
    "version": 2,
    "builds": [
        {
            "src": "pesonal_finance_manager/personal_finance_manager/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.11.4"
            }
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": { "distDir": "staticfiles_build" }
        }
    ],
    "routes": [
        { "src": "/static/(.*)", "dest": "/static/$1" },
        { "src": "/(.*)", "dest": "pesonal_finance_manager/pesonal_finance_manager/wsgi.py" }
    ]
}
